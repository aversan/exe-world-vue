{
  "name": "exe-world-vue",
  "version": "1.0.0",
  "description": "Exe World project",
  "author": "Alexandr Averyanov",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "prebuild": "npm run icons",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest",
    "icons": "vsvg -s ./assets/icons -t ./components/icons --svgo svgo.js --es6"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.0",
    "nuxt": "^2.14.0"
  },
  "devDependencies": {
    "@nuxtjs/device": "^1.2.7",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^2.1.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@tailwindcss/typography": "^0.2.0",
    "@types/node": "^14.6.4",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "css-loader": "^4.2.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "nuxt-purgecss": "^1.0.0",
    "nuxt-webfontloader": "^1.1.0",
    "prettier": "^2.0.5",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "swiper": "^6.2.0",
    "tailwindcss": "^1.8.2",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "url-loader": "^4.1.0",
    "vsvg": "^1.9.1",
    "vue-awesome-swiper": "^4.1.1",
    "vue-fragment": "^1.5.1",
    "vue-jest": "^3.0.4",
    "vue-mq": "^1.0.1",
    "vue-svg-loader": "^0.16.0",
    "vue-svgicon": "^3.2.8"
  }
}
